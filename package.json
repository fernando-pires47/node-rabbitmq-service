{
  "name": "rabbitmq-service",
  "version": "1.0.0",
  "description": "Super simple RabbitMQ consumer",
  "author": "Fernando Pires",
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "start:docker": "sudo docker compose -f docker-compose.dev.yml up --build",
